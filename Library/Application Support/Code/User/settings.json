{
  "editor.bracketPairColorization.enabled": true,
  "editor.wrappingIndent": "same",
  "breadcrumbs.enabled": true,
  "gitlens.codeLens.authors.enabled": false,
  "editor.fontLigatures": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "gitlens.codeLens.recentChange.enabled": false,
  "editor.suggestSelection": "first",
  "workbench.editor.revealIfOpen": true,
  "files.insertFinalNewline": true,
  "editor.fontFamily": "Hack Nerd Font",
  "editor.accessibilitySupport": "off",
  "editor.tabSize": 2,
  "errorLens.enabled": false,
  "editor.fontWeight": "500",
  "emmet.showSuggestionsAsSnippets": true,
  "workbench.startupEditor": "newUntitledFile",
  // "editor.wordWrapColumn": 80,
  "editor.dragAndDrop": false,
  "indentRainbow.colors": [
    "rgba(16,16,16,0.1)",
    "rgba(16,16,16,0.3)",
    "rgba(16,16,16,0.6)",
    "rgba(16,16,16,0.4)",
    "rgba(16,16,16,0.2)"
  ],
  "emmet.triggerExpansionOnTab": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "errorLens.excludeBySource": ["cSpell"],
  "terminal.integrated.defaultProfile.osx": "zsh",
  "editor.inlineSuggest.enabled": true,
  // https://stackoverflow.com/questions/45916181/vs-code-how-to-keep-two-projects-open-in-tabs-instead-of-two-windows/71657525#71657525
  "window.nativeTabs": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.minimap.enabled": false,
  "editor.formatOnPaste": false,
  "search.exclude": {
    "/venv": true,
    "/bower_components": true,
    "yarn.lock": true,
    "/.next": true,
    "/node_modules": true,
    "/env": true
  },
  "editor.formatOnSave": true,
  "gruvboxMaterial.darkContrast": "hard",
  "errorLens.delay": 2000,
  "eslint.format.enable": true,
  "files.watcherExclude": {
    "/venv/": true,
    "/.next": true,
    "/.git/subtree-cache/": true,
    "env-*": true,
    "/node_modules/": true,
    "yarn.lock": true,
    "/.git/objects/": true,
    "/env/": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.trimTrailingWhitespace": true,
  "vsicons.dontShowNewVersionMessage": true,
  "typescript.suggest.paths": false,
  "workbench.iconTheme": "vscode-icons",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "security.workspace.trust.untrustedFiles": "open",
  "cSpell.userWords": [
    "algo",
    "behaviour",
    "bodyparser",
    "browserslist",
    "chargebee",
    "deduping",
    "dotfiles",
    "fadeinout",
    "fiability",
    "formik",
    "fuzznimp",
    "gruvbox",
    "leafcrop",
    "lightstep",
    "meteoblue",
    "middlewares",
    "multimodel",
    "neighbour",
    "neighbours",
    "nextjs",
    "oluane",
    "openapi",
    "opentelemetry",
    "organisation",
    "organisations",
    "prefetch",
    "procfile",
    "remoteaddr",
    "rgba",
    "roboto",
    "sencrop",
    "sigfox",
    "taiko",
    "timeseries",
    "unmarshall",
    "uuidv",
    "vercel",
    "zstyle",
    "rgba",
    "solarcrop",
    "DATETIME",
    "Luxon",
    "antho",
    "tfstate"
  ],
  "extensions.ignoreRecommendations": true,
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground1": "#8b4513",
    "editorBracketHighlight.foreground2": "#228b22",
    "editorBracketHighlight.foreground3": "#cc7722"
  },
  "http.systemCertificates": false,
  "explorer.confirmDelete": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "explorer.confirmDragAndDrop": false,
  // "editor.wordWrap": "wordWrapColumn",
  "git.enableSmartCommit": true,
  "git.mergeEditor": true,
  "editor.renderWhitespace": "all",
  "editor.cursorStyle": "block",
  "diffEditor.codeLens": true,
  "editor.linkedEditing": true,
  "git.autofetch": true,
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "svgPreview.autoOpen": false,
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "githubPullRequests.pullBranch": "never",
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "presentationMode.zoomLevel": 4,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]",
    "NOTE"
  ],
  "window.titleBarStyle": "custom",
  "apc.electron": {
    "opacity": 0.9,
    "transparent": true,
    "backgroundColor": "#00000000",
    "vibrancy": "tooltip",
    "titleBarStyle": "hiddenInset"
    // "visualEffectState": "active"
  },
  // "apc.header": {
  //   "height": 25,
  //   "fontSize": 12
  // },
  // "apc.listRow": {
  //   "height": 19,
  //   "fontSize": 11
  // },
  "apc.stylesheet": {
    "body": {
      "background-blend-mode": "multiply",
      "background-repeat": "no-repeat"
    }
  },
  "workbench.colorTheme": "Gruvbox Material Dark",
  "vim.highlightedyank.enable": true,
  "vim.sneak": true,
  "vim.leader": "space",
  "editor.autoClosingBrackets": "never",
  "vim.smartRelativeLine": true,
  "editor.cursorSurroundingLines": 20,
  "editor.lineNumbers": "on",
  "window.zoomLevel": 1,
  "workbench.activityBar.location": "top",
  "window.commandCenter": false,
  "inlineChat.mode": "live",
  "diffEditor.hideUnchangedRegions.enabled": true
}
