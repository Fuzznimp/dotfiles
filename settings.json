{
  "editor.bracketPairColorization.enabled": true,
  "editor.wrappingIndent": "same",
  "breadcrumbs.enabled": true,
  "gitlens.codeLens.authors.enabled": false,
  "editor.fontLigatures": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "gitlens.codeLens.recentChange.enabled": false,
  "editor.suggestSelection": "first",
  "workbench.editor.revealIfOpen": true,
  "files.insertFinalNewline": true,
  "editor.fontFamily": "Hack Nerd Font",
  "editor.accessibilitySupport": "off",
  "editor.tabSize": 2,
  "errorLens.enabled": false,
  "editor.fontWeight": "500",
  "emmet.showSuggestionsAsSnippets": true,
  "workbench.startupEditor": "newUntitledFile",
  "editor.wordWrapColumn": 80,
  "editor.dragAndDrop": false,
  "indentRainbow.colors": [
    "rgba(16,16,16,0.1)",
    "rgba(16,16,16,0.3)",
    "rgba(16,16,16,0.6)",
    "rgba(16,16,16,0.4)",
    "rgba(16,16,16,0.2)"
  ],
  "zenMode.hideTabs": false,
  "emmet.triggerExpansionOnTab": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "errorLens.excludeBySource": ["cSpell"],
  "terminal.integrated.defaultProfile.osx": "zsh",
  "editor.inlineSuggest.enabled": true,
  "window.nativeTabs": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.minimap.enabled": false,
  "editor.formatOnPaste": false,
  "vscodeGoogleTranslate.preferredLanguage": "French",
  "search.exclude": {
    "/venv": true,
    "/bower_components": true,
    "yarn.lock": true,
    "/.next": true,
    "/node_modules": true,
    "/env": true
  },
  "editor.formatOnSave": true,
  "gruvboxMaterial.darkContrast": "hard",
  "errorLens.delay": 2000,
  "diffEditor.ignoreTrimWhitespace": false,
  "eslint.format.enable": true,
  "files.watcherExclude": {
    "/venv/": true,
    "/.next": true,
    "/.git/subtree-cache/": true,
    "env-*": true,
    "/node_modules/": true,
    "yarn.lock": true,
    "/.git/objects/": true,
    "/env/": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.trimTrailingWhitespace": true,
  "vsicons.dontShowNewVersionMessage": true,
  "typescript.suggest.paths": false,
  "workbench.iconTheme": "vscode-icons",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "security.workspace.trust.untrustedFiles": "open",
  "workbench.colorTheme": "Gruvbox Material Dark",
  "cSpell.userWords": [
    "meteoblue",
    "multimodel",
    "algo",
    "behaviour",
    "bodyparser",
    "browserslist",
    "chargebee",
    "deduping",
    "dotfiles",
    "fadeinout",
    "fiability",
    "formik",
    "fuzznimp",
    "leafcrop",
    "lightstep",
    "middlewares",
    "neighbour",
    "neighbours",
    "nextjs",
    "oluane",
    "openapi",
    "opentelemetry",
    "organisation",
    "organisations",
    "prefetch",
    "procfile",
    "remoteaddr",
    "roboto",
    "sencrop",
    "sigfox",
    "taiko",
    "timeseries",
    "unmarshall",
    "uuidv",
    "vercel"
  ],
  "turboConsoleLog.addSemicolonInTheEnd": true,
  "extensions.ignoreRecommendations": true,
  "workbench.colorCustomizations": {
    "editorBracketHighlight.foreground2": "#228b22",
    "editorBracketHighlight.foreground1": "#8b4513",
    "editorBracketHighlight.foreground3": "#cc7722"
  },
  "http.systemCertificates": false,
  "explorer.confirmDelete": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
    // "source.sortImports": true
  },
  "explorer.confirmDragAndDrop": false,
  "editor.wordWrap": "wordWrapColumn",
  "git.enableSmartCommit": true,
  "git.mergeEditor": true,
  "window.commandCenter": true,
  "editor.renderWhitespace": "all",
  "Lua.telemetry.enable": true,
  "window.zoomLevel": 1,
  "editor.cursorStyle": "block",
  "diffEditor.codeLens": true,
  "editor.linkedEditing": true,
  "git.autofetch": true,
  "[sql]": {
    "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
  },
  "svgPreview.autoOpen": false
}
